<?xml version="1.0" encoding="utf-8"?>
<config>
    <appConfiguration>
        <help>www.rp3.com.ec</help>
        <allowSettingsOnAut>TRUE</allowSettingsOnAut>
        <allowHelpOnAut>TRUE</allowHelpOnAut>
        <imagesFolder>/Content/AgendaComercial/img/clientes/</imagesFolder>
        <cancelAlarmTime>22H00</cancelAlarmTime>
        <forgotPassword>http://54.94.162.88/MarketForce/Security/Account/RequestingPasswordRecovery</forgotPassword>
    </appConfiguration>
    <dataBaseConfiguration>       
       <name>Rp3MarketForce.db</name>
       <version>1</version>
    </dataBaseConfiguration>        
    <webServiceConfiguration>                
        <webServiceData>
            <name>Core</name>
            <url>[@server:http://54.94.162.88/marketforce]/api</url>
            <type>REST</type>            
            <oAuthClientId>android-app-marketforce</oAuthClientId>
            <oAuthClientSecret>rp3-marketforce2014</oAuthClientSecret>
            <methods>
                <method>
                    <name>SignIn</name>
                    <action>account/signin</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>GetGeopoliticalStructures</name>
                    <action>General/GetGeopoliticalStructuresView?FecModParameter=@FecModParameter</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetGeneralValues</name>
                    <action>General/GetGeneralValues</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetIdentificationTypes</name>
                    <action>General/GetIdentificationTypes</action>
                    <webMethod>GET</webMethod>
                </method>      
            </methods>     
        </webServiceData>
           <webServiceData>
            <name>MartketForce</name>
            <url>[@server:http://54.94.162.88/marketforce]/api</url>
            <type>REST</type>            
            <oAuthClientId>android-app-marketforce</oAuthClientId>
            <oAuthClientSecret>rp3-marketforce2014</oAuthClientSecret>
            <methods>
                <method>
                    <name>GetClientes</name>
                    <action>cliente/getclientes?ultimaactualizacion=@ultimaactualizacion</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetServer</name>
                    <action>application/getclient?applicationid=@applicationid&amp;validationCode=@validationCode</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>CreateCliente</name>
                    <action>cliente/create</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>GetParametros</name>
                    <action>parametro/getall</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>SetFotos</name>
                    <action>cliente/setfotocliente</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>SetMediaAgenda</name>
                    <action>agenda/setagendamedia</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>Agenda</name>
                    <action>agenda/get?fechainicio=@fechainicio&amp;fechafin=@fechafin&amp;ultimaactualizacion=@ultimaactualizacion</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>UpdateAgenda</name>
                    <action>agenda/update</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>UpdateAgendaFull</name>
                    <action>agenda/updatefull</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>ReprogramarAgenda</name>
                    <action>agenda/reprogramar</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>MotivoNoVisitaAgenda</name>
                    <action>agenda/SetMotivoNoGestion</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>InsertarAgenda</name>
                    <action>agenda/insert</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>GetTipoClientes</name>
                    <action>tipocliente/get</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetCanales</name>
                    <action>canal/get</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetAgente</name>
                    <action>agente/getAgente</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetResumenGestionAgentes</name>
                    <action>gestion/getresumengestionagentes?idagentesupervisor=@idagentesupervisor</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetFoto</name>
                    <action>cliente/getfoto?idcliente=@idcliente</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>GetTareas</name>
                    <action>tarea/get</action>
                    <webMethod>GET</webMethod>
                </method>
                <method>
                    <name>SetUbicaciones</name>
                    <action>agente/setubicaciones</action>
                    <webMethod>POST</webMethod>
                </method> 
                <method>
                    <name>SetUbicacion</name>
                    <action>agente/setubicacion?latitud=@latitud&amp;longitud=@longitud</action>
                    <webMethod>POST</webMethod>
                </method>
                <method>
                    <name>UpdateCliente</name>
                    <action>cliente/update</action>
                    <webMethod>POST</webMethod>
                </method> 
                <method>
                    <name>UpdateClienteFull</name>
                    <action>cliente/updatefull</action>
                    <webMethod>POST</webMethod>
                </method>               
            </methods>     
        </webServiceData>
           <webServiceData>
            <name>ServerVerification</name>
            <url>http://54.94.162.88/rp3deploy/api</url>
            <type>REST</type>            
            <methods>
                <method>
                    <name>GetServer</name>
                    <action>application/getclient?applicationid=@applicationid&amp;validationCode=@validationCode</action>
                    <webMethod>GET</webMethod>
                </method>              
            </methods>     
        </webServiceData>
    </webServiceConfiguration>
</config>